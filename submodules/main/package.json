{
  "name": "@mobk/main",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:homolog": "react-native run-android --variant=homologdebug",
    "android:homolog:newarch": "react-native run-android --active-arch-only --variant=homologdebug",
    "android:production": "react-native run-android --variant=productiondebug",
    "android:production:newarch": "react-native run-android --active-arch-only --variant=productiondebug",
    "bundle:view": "visualize-bundle",
    "ios:pods": "cd ios && rm -rf Podfile.lock && pod install --repo-update",
    "ios:pods:newarch": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install --repo-update",
    "ios:clean": "cd ios/ && xcodebuild -workspace Original.xcworkspace -configuration Debug -scheme Original clean",
    "ios:homolog": "yarn ios:clean && react-native run-ios --scheme 'OriginalHomolog'",
    "ios:production": "yarn ios:clean && react-native run-ios --scheme 'OriginalProduction'",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "start": "react-native start"
  },
  "resolutions": {
    "@babel/eslint-parser": "^7.19.1",
    "@mobk/engine": "./node_modules/@mobk/engine",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.0.5"
  },
  "dependencies": {
    "@mobk/engine": "./node_modules/@mobk/engine",
    "@mobk/module-example": "./node_modules/@mobk/module-example",
    "@react-native-firebase/analytics": "^16.4.3",
    "@react-native-firebase/app": "^16.4.3",
    "@react-native-firebase/crashlytics": "^16.4.3",
    "@react-native-firebase/dynamic-links": "^16.4.3",
    "@react-native-firebase/in-app-messaging": "^16.4.3",
    "@react-native-firebase/messaging": "^16.4.3",
    "@react-native-firebase/perf": "^16.4.3",
    "@react-native-firebase/remote-config": "^16.4.3",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "react": "^18.1.0",
    "react-native": "^0.70.4",
    "react-native-bundle-splitter": "^2.2.1",
    "react-native-config": "^1.4.11",
    "react-native-get-random-values": "^1.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "zustand": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/runtime": "^7.12.5",
    "@commitlint/config-conventional": "^17.1.0",
    "@evilmartians/lefthook": "^1.1.3",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "commitlint": "^17.1.2",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "metro-minify-esbuild": "^0.2.0",
    "metro-react-native-babel-preset": "0.72.3",
